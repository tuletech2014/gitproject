      scope="request"
      type="org.springframework.web.struts.DelegatingActionProxy"
      validate="false">
      <forward
        name="tisave"
        path="/Carmack/JsonFactory/TruckInfoSave.jsp"
        contextRelative="true" />
    </action>
    <action
      path="/CarmacktruckInfoUpdateOk"
      scope="request"
      type="org.springframework.web.struts.DelegatingActionProxy"
      validate="false" >     
