      validate="false" >
      <forward
        name="mobilllist"
        path="/Moster/JsonFactory/MoBillList.jsp"
        contextRelative="true" />
      
      
    </action>
    <action
      path="/MocargoBillInfo"
      type="org.springframework.web.struts.DelegatingActionProxy"
      validate="false" >
      <forward
        name="mocargobill"
        path="/Moster/JsonFactory/MoCVBillID.jsp"
        contextRelative="true" />
    </action>

