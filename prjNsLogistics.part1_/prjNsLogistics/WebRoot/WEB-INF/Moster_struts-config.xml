      type="org.springframework.web.struts.DelegatingActionProxy"
      validate="false" >
      <forward
        name="mocargoselectupdate"
        path="/Moster/JsonFactory/MoCargoSelectUpdate.jsp"
        contextRelative="true" />
    </action>


















